alue={experience[i].employment_period_month}\n                                                                    required\n                                                                />\n                                                            </div>\n                                                        </div>\n\n                                                        <div className=\"col-lg-12\">\n                                                            <div className=\"form-group\">\n                                                                <label>Description</label>\n                                                                <textarea \n                                                                    className=\"form-control\" \n                                                                    rows=\"4\"\n                                                                    name=\"description\" \n                                                                    onChange={(e) => handleExperience(e,i)}\n                                                                    required\n                                                                    value={experience[i].description}\n                                                                >\n                                                                </textarea>\n                                                            </div>\n                                                        </div>\n                                                    </div>                                                    \n                                                </div>\n                                            </div>\n                                        \n                                        </React.Fragment>\n                                        )\n                                    })}\n\n                                <div className=\"row\">\n                             